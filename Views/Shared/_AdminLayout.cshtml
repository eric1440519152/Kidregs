@using Microsoft.EntityFrameworkCore.Storage
<!DOCTYPE html>
<html lang="cn">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="幼儿园新生信息采集系统" name="description" />
    <meta content="He Zeen" name="author" />
    <meta content="Coderthemes" name="Theme From" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="/images/favicon.ico">
    <title>@ViewData["Title"] - @ViewBag.SystemOptions.SiteName</title>

    @RenderSection("PluginsCSS")

    <!-- App css -->
    <link href="/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/css/app.min.css" rel="stylesheet" type="text/css" />

</head>

<body>
    <!-- Navigation Bar-->
    <header id="topnav">
        <!-- Topbar Start -->
        <div class="navbar-custom">
            <div class="container-fluid">
                <ul class="list-unstyled topnav-menu float-right mb-0">
                    <li class="dropdown notification-list">

                    </li>
                </ul>
                <!-- LOGO -->
                <div class="logo-box">
                    <a href="@ViewBag.SystemOptions.Domain" class="logo text-center">
                        <span class="logo-lg">
                            <img src="/images/logo-light.png" alt="" height="16">
                            <!-- <span class="logo-lg-text-light">UBold</span> -->
                        </span>
                        <span class="logo-sm">
                            <!-- <span class="logo-sm-text-dark">U</span> -->
                            <img src="/images/logo-sm.png" alt="" height="24">
                        </span>
                    </a>
                </div>
            </div>
            <!-- end container-fluid-->
        </div>
        <!-- end Topbar -->
        <!-- end navbar-custom -->
    </header>
    <!-- End Navigation Bar-->
    <!--==============================================================-->
    <!-- Start Page Content here -->
    <!--==============================================================-->
    @RenderBody()

    <!-- end wrapper -->
    <!--==============================================================-->
    <!-- End Page content -->
    <!--==============================================================-->
    <!-- Footer Start -->
    <footer class="footer">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12 text-center">
                    @ViewBag.SystemOptions.CopyrightMessage
                </div>
            </div>
        </div>
    </footer>
    <!-- end Footer -->
    <!-- Vendor js -->
    <script src="/js/vendor.min.js"></script>

    @RenderSection("PluginsJS",false)

    @RenderSection("InitJS",false)

    <!-- App js -->
    <script src="/js/app.min.js"></script>

    @RenderSection("CustomizeJS",false)
    @RenderSection("CustomizeCSS",false)
</body>
</html>
